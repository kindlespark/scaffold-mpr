{
  "application": {
    "title": "MPR v0.0.2",

    "theme": "bootswatch-united",
    "copy_files": [
      { "source": "public", "dest": "PUBLIC_DIR/" },
      { "source": "client", "dest": "CLIENT_DIR/" },
      { "source": "settings-dev.json", "dest": "OUTPUT_DIR/" }
    ],
    "client_startup_source_file": "imports/startup/client-startup.js",

    "login_with_google": true,

    "collections": {
      "$import": "collections/collections.json#/collections"
    },
    "queries": {
      "$import": "queries/queries.json#/queries"
    },
    "private_zone": {
      "$import": "pages/private_zone.json#/private_zone"
    },
    "public_zone": {
      "$import": "pages/public_zone.json#/public_zone"
    }
  }
}
